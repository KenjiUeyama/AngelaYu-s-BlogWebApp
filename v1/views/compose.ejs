<%- include("partials/header") %>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" value="" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Content</label>
    <textarea class="form-control" name="postBody" rows="3" cols="40" autocomplete="off"></textarea>
  </div>
  <button class="btn btn-primary" type="submit">Post</button>
</form>



<%- include("partials/footer") %>
